<% if (theme.links.length){ %>
 <div class="widget-wrap">
    <h3 class="widget-title">Links</h3>
	<ul class="widget">
	<% for (var i in theme.links){ %>
		<li><a href="<%- theme.links[i].url %>" title="<%- theme.links[i].intro %>" target="_blank"]);"><%= theme.links[i].title %></a></li>
	<% } %>
	</ul>
</div>
<% } %>
